<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.4</TargetFramework>
    <Version>0.8.0</Version>
    <Authors>Dima Stadub</Authors>
    <Company>Dima Stadub</Company>
    <Description>Spread of classes, used in different projects as a shared component.
      Service locator - service locator implementation with type mapping capabilities.
      Object Mapper -service locator implementation with type mapping capabilities.</Description>
    <PackageLicenseUrl>https://opensource.org/licenses/BSD-3-Clause</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/stadub/Net.Utils</PackageProjectUrl>
    <Copyright>(C) Dima Stadub</Copyright>
    <PackageIconUrl>https://raw.githubusercontent.com/stadub/Net.Utils/master/favicon.ico</PackageIconUrl>
    <RepositoryUrl>https://github.com/stadub/Net.Utils</RepositoryUrl>
    <PackageTags>IoC Helpers Utils Mapper ServiceLocator</PackageTags>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="favicon.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.ComponentModel.TypeConverter" Version="4.3.0" />
  </ItemGroup>

</Project>